<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="900.0" minWidth="1030.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="900.0">
         <items>
            <SplitPane dividerPositions="0.5" orientation="VERTICAL">
               <items>
                  <AnchorPane>
                     <children>
                        <Label alignment="CENTER" layoutX="3.0" layoutY="6.0" text="BOAR BREEDING RECORD" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="0.0">
                           <font>
                              <Font name="System Bold" size="18.0" />
                           </font>
                        </Label>
                        <Label fx:id="BreedReportStartEnd" alignment="CENTER" contentDisplay="CENTER" layoutX="2.0" layoutY="61.0" prefHeight="45.0" text="Breeding Period: From 00/00/0000 to 00/00/0000" AnchorPane.leftAnchor="2.0" AnchorPane.rightAnchor="1.0">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Label>
                        <Label fx:id="BoarNo" alignment="CENTER" layoutX="2.0" layoutY="36.0" prefHeight="30.0" text="Boar No: 00000" AnchorPane.leftAnchor="2.0" AnchorPane.rightAnchor="1.0">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                     </children>
                  </AnchorPane>
                  <AnchorPane stylesheets="@text.css">
                     <children>
                        <TableView id="BreedReportTable" fx:id="BreedReportTable" layoutX="1.0" prefHeight="600.0" prefWidth="1050.0" stylesheets="@text.css" />
                     </children>
                  </AnchorPane>
               </items>
            </SplitPane>
            <SplitPane dividerPositions="0.5" minHeight="225.0">
               <items>
                  <TextArea id="BreedingPerfText" fx:id="BreedingPerfText" editable="false" minHeight="225.0" prefWidth="500.0" text=" ">
                     <font>
                        <Font size="16.0" />
                     </font>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </TextArea>
                  <TextArea id="FarrowingPerfText" fx:id="FarrowingPerfText" editable="false" minHeight="225.0" prefWidth="500.0" text=" ">
                     <font>
                        <Font size="16.0" />
                     </font>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </TextArea>
               </items>
            </SplitPane>
         </items>
      </SplitPane>
   </children>
</AnchorPane>
